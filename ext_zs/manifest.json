{
  "manifest_version": 2,

  "name": "Z-Search",
  "description": "Provide a quick way to search in kfc.",
  "version": "0.1",
  "icons": { "16": "img/logo.png",
             "48": "img/logo.png",
             "128": "img/logo.png" },
  "permissions" : ["tabs"],
  
  "browser_action": {
    "default_icon": "img/z.png",
    "default_title": "Z-Search",
    "default_popup": "popup.html"
  }
}
